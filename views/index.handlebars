<div class="container">
    <section id="page-name">
    <img src="data:image/png;base64," alt="taco-img" class="center">
    <h1>Eat-Da-Taco!</h1>
    </section>

    <section class="row">
        <div class="col-md-7 col-left" id="not-devoured">
            <table>
                <p id="title"> Ready to Eat!</p>
                {{#each tacos}}
                    {{#unless devoured}}
                        {{> taco/taco-block}}
                    {{/unless}}
                {{/each}}
            </table>
        </div>
        <div class="col-md-5 col-right" id="devoured">    
            <table>
                <p id="title">Devoured Tacos</p>
                {{#each tacos}}
                    {{#if devoured}}
                        <div id="dev-taco-name">{{taco_name}}</div>
                    {{/if}}
                {{/each}}
            </table>
        </div>          
    </section>

<div>
    <form class="create-form">
        <div class="form-group">
            <label for="name"></label>
            <textarea id="taco_name" name="taco_name" rows="4" cols="40" placeholder=""></textarea>
        </div>
        <button type="submit" id="taco_button">Add Taco</button>
    </form>  
</div>
   
</div>